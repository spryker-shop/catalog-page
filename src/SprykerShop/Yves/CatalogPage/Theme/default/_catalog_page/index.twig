{% block breadcrumbItems %}
    <li><a href="/">{{ 'global.home' | trans }}</a></li>
    {% include '@CatalogPage/_partials/category-breadcrumb.twig' with {categoryNode: _view.data.category} only %}
{% endblock %}

{% block title %}
    <h3>{{ _view.data.category.name }}</h3>
{% endblock %}

{% block content %}
    {% include "@CatalogPage/_partials/catalog.twig" with {paginationPath: app.request.getPathInfo()} %}
{% endblock %}
